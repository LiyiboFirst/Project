/* pages/baidu/0002/baidu.wxss */
.upload-img{
  width: 70%;
  border-radius: 98rpx; 
  background-color: #6666FF;
  color: #ffffff;
  position:fixed;
  bottom:20rpx;
  left:15%;
}
.data{
  font-size: 40rpx;
  color: #333333;
  margin-bottom: 20rpx;
  margin-left: 3%;
}

.left{
  width: 100%;
  display: flex;
  margin-bottom: 40rpx;
}

.text{
  font-size: 30rpx;
  color:#666666;
  width: 97%;
}


.click image{
 width: 30rpx;
 height: 30rpx;
}

.table{
  width: 94%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  margin-left: 3%;
  flex-wrap: wrap;
  margin-bottom: 60rpx;
}
 
.tr{
  display: flex;
  width: 100%;
  border:2rpx solid #ccc;
  margin-bottom: 20rpx;
}

.td{
  display: flex;
  width: 100%;
  margin-left: 10rpx;
  margin-top: 20rpx;
  flex-wrap: wrap;
  flex-direction: row;
  margin-bottom: 20rpx;
}

.bg-c{
  width: 30%;
  font-size: 30rpx;

}

.bg-v{
 width: 70%;
 font-size: 30rpx;
 color:cornflowerblue;
 
}



.photo { 
display: flex;
justify-content: center;
margin-top: 10rpx;
margin-bottom: 10rpx;
}


