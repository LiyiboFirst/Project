<!--pages/lonton/0002/lonton.wxml-->
<view class="photo">
<image src="{{tempFilePaths}}" mode="aspectFit" ></image>
</view>
   
<view class="data">识别结果</view>

<view class="table">

 
<view class="tr bg-g" >
       <view class="td bg-c">发票号码</view>
       <view class="td bg-v" >{{list[0].value}}</view>
    </view>
     <view class="tr bg-g" >
       <view class="td bg-c">发票代码</view>
       <view class="td bg-v" >{{list[1].value}}</view>
    </view>
     <view class="tr bg-g" >
       <view class="td bg-c">不含税金额</view>
       <view class="td bg-v" >{{list[3].value}}</view>
    </view>

     <view class="tr bg-g" >
       <view class="td bg-c">开票日期</view>
       <view class="td bg-v" >{{list[2].value}}         </view>
     </view>
     
     <view class="tr bg-g" >
       <view class="td bg-c">校验码</view>
       <view class="td bg-v" >{{list[4].value}}</view>
    </view>
     
    
      
  </view>

  <button class="upload-img"  bindtap="choose">上传增值税发票</button>

  

   


 

 

 

 



   

